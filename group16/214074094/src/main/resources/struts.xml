<?xml version="1.0" encoding="UTF-8"?>
<struts>
    <action name="login" class="study.coderising.litestruts.LoginAction">
        <result result="success">/jsp/homepage.jsp</result>
        <result result="fail">/jsp/showLogin.jsp</result>
    </action>
    <action name="logout" class="study.coderising.litestruts.LogoutAction">
        <result result="success">/jsp/welcome.jsp</result>
        <result result="error">/jsp/error.jsp</result>
    </action>
</struts>